{
  "manifest_version": 3,
  "name": "Xlsx to JSON",
  "version": "0.0.1",

  "action": {
    "default_title": "Xlsx to JSON",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content_script.js"]
    }
  ],
  "description": "Change xlsx data to json",
  "options_page": "options.html",
  "permissions": [
    "storage",
    "tabs"
  ]
}